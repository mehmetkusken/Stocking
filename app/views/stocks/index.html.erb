<p id="notice"><%= notice %></p>

<h1>Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Hisse</th>
      <th>Hacimstr</th>
      <th>Hacim</th>
      <th>Lastpricestr</th>
      <th>Lastprice</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stocks.each do |stock| %>
      <tr>
        <td><%= stock.name %></td>
        <td><%= stock.hisse %></td>
        <td><%= stock.hacimstr %></td>
        <td><%= stock.hacim %></td>
        <td><%= stock.lastpricestr %></td>
        <td><%= stock.lastprice %></td>
        <td><%= stock.rate %></td>
        <td><%= link_to 'Show', stock %></td>
        <td><%= link_to 'Edit', edit_stock_path(stock) %></td>
        <td><%= link_to 'Destroy', stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock', new_stock_path %>
